node {
    stage('SCM') {
	echo 'Gathering code from version control'
	git branch: '${branch}', url: 'https://github.com/Ishan0075/JenkinsGroovy.git'
    }
    stage('Build') {
        echo 'Building....'
		bat 'dotnet --version'
		bat 'dotnet restore --force'
		bat 'dotnet build ConsoleApp1'
		echo 'Build new feature'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }

 post { 
        always { 
            cleanWs()
        }
    }

}
